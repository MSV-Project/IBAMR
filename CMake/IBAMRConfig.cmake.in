#-----------------------------------------------------------------------------
#
# IBAMRConfig.cmake - IBAMR CMake configuration file for external projects.
#
# This file is configured by IBAMR and used by the UseIBAMR.cmake module
# to load IBAMR's settings for an external project.

# The set of IBAMR libraries
SET(IBAMR_LIBRARIES "@IBAMR_LIBRARIES@")
SET(IBAMR_EXTERNAL_LIBRARIES "@IBTK_EXTERNAL_LIBRARIES@")

# The IBAMR include file directories.
SET(IBAMR_INCLUDE_DIRS "@IBAMR_INCLUDE_DIRS@")

# The IBAMR library directories.
SET(IBAMR_LIBRARY_DIRS "@IBAMR_LIBRARY_DIRS@")

# The C and C++ flags added by IBAMR to the cmake-configured flags.
SET(IBAMR_C_FLAGS "@IBAMR_C_FLAGS@")
SET(IBAMR_CXX_FLAGS "@IBAMR_CXX_FLAGS@")
SET(IBAMR_Fortran_FLAGS "@IBAMR_Fortran_FLAGS@")
SET(IBAMR_EXE_LINKER_FLAGS "@CMAKE_EXE_LINKER_FLAGS@")
SET(IBAMR_SHARED_LINKER_FLAGS "@CMAKE_SHARED_LINKER_FLAGS@")
SET(IBAMR_MODULE_LINKER_FLAGS "@CMAKE_MODULE_LINKER_FLAGS@")

# The IBAMR version number
SET(IBAMR_MAJOR_VERSION "@IBAMR_MAJOR_VERSION@")
SET(IBAMR_MINOR_VERSION "@IBAMR_MINOR_VERSION@")
SET(IBAMR_BUILD_VERSION "@IBAMR_BUILD_VERSION@")

# The location of the UseIBAMR.cmake file.
SET(IBAMR_USE_FILE "@IBAMR_USE_FILE@")

# CMake extension module directory and macro file.
SET(IBAMR_CMAKE_DIRS "@IBAMR_CMAKE_DIR@")

# IBAMR Configuration options.
SET(IBAMR_BUILD_SHARED_LIBS "@BUILD_SHARED_LIBS@")

# The MPI configuration
SET(IBAMR_MPIEXEC "@MPIEXEC@")
SET(IBAMR_MPI_MAX_NUMPROCS "@MPI_MAX_NUMPROCS@")
SET(IBAMR_MPI_NUMPROC_FLAG "@MPIEXEC_NUMPROC_FLAG@")
SET(IBAMR_MPI_POSTFLAGS "@MPIEXEC_POSTFLAGS@")
SET(IBAMR_MPI_PREFLAGS "@MPIEXEC_PREFLAGS@")
SET(IBAMR_MPI_INCLUDE_DIR "@MPI_INCLUDE_PATH@")
SET(IBAMR_MPI_LIBRARIES "@MPI_LIBRARY@;@MPI_EXTRA_LIBRARY@")
# The Doxygen configuration.
SET(IBAMR_DOXYGEN_DIR "@IBAMR_DOXYGEN_DIR@")

# Relative install paths in the IBAMR install tree
SET(IBAMR_INSTALL_BIN_DIR "@IBAMR_INSTALL_BIN_DIR@")
SET(IBAMR_INSTALL_INCLUDE_DIR "@IBAMR_INSTALL_INCLUDE_DIR@")
SET(IBAMR_INSTALL_LIB_DIR "@IBAMR_INSTALL_LIB_DIR@")
SET(IBAMR_INSTALL_PACKAGE_DIR "@IBAMR_INSTALL_PACKAGE_DIR@")

# The IBAMR targets file.
IF(EXISTS "@IBAMR_TARGETS_FILE@")
  INCLUDE("@IBAMR_TARGETS_FILE@")
ENDIF()

# The IBTK targets file.
IF(EXISTS "@IBTK_TARGETS_FILE@")
  INCLUDE("@IBTK_TARGETS_FILE@")
ENDIF()
